{
    "bitwise": true,
    "curly": true,
    "eqeqeq": true,
    "es3": false,
    "es5": false,
    "forin": true,
    "freeze": true,
    "funcscope": true,
    "futurehostile": true,
    "globals": false,
    "indent": 4,
    "iterator": false,
    "latedef": "nofunc",
    "maxcomplexity": 8,
    "maxdepth": 5,
    "maxerr": 50,
    "maxparams": 10,
    "maxstatements": 100,
    "noarg": true,
    "nocomma": true,
    "nonbsp": true,
    "nonew": true,
    "notypeof": false,
    "shadow": false,
    "singleGroups": false,
    "strict": true,
    "undef": true,
    "unused": "vars",
    "varstmt": false,

    // relaxing options
    "asi": false,
    "boss": false,
    "debug": false,
    "elision": false,
    "eqnull": true,
    "esnext": false,
    "evil": false,
    "expr": false,
    "globalstrict": false,
    "lastsemic": false,
    "loopfunc": true,
    "moz": false,
    "multistr": false,
    "noyield": false,
    "plusplus": false,
    "proto": false,
    "scripturl": false,
    "supernew": false,
    "validthis": false,
    "withstmt": false,

    // environments
    "browser": true,
    "devel": true,
    "node": true
}
